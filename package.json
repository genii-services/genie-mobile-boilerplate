{
	"name": "genie-mobile-boilerplate",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint .",
		"postinstall": "cp -R modules/* node_modules/ && npx jetify",
		"clean-android": "cd android && ./gradlew clean && cd ..",
		"clear": "./scripts/clear.sh",
		"clone": "./scripts/clone.sh",
		"create": "./scripts/create.sh",
		"android-power": "adb shell input keyevent KEYCODE_POWER",
		"android-wakeup": "adb shell input keyevent KEYCODE_WAKEUP",
		"android-log-clear": "adb logcat -c",
		"android-log-error": "adb logcat '*:E'",
		"android-log": "adb logcat *:S ReactNative:V ReactNativeJS:V",
		"minor-up": "yarn version --minor && react-native-version --never-amend",
		"patch-up": "yarn version --patch && react-native-version --never-amend",
		"build": "./scripts/build.sh",
		"i": "./scripts/install.sh",
		"u": "./scripts/uninstall.sh",
		"pod": "cd ios && pod install && cd ..",
		"gen-release-key": "cd android/app && keytool -genkey -v -keystore release.keystore -alias release-key-alias -keyalg RSA -keysize 2048 -validity 10000 && cd ../..",
		"hi": "./scripts/hi.sh",
		"bye": "./scripts/bye.sh"
	},
	"dependencies": {
		"@react-native-community/async-storage": "^1.12.0",
		"@react-native-community/datetimepicker": "^3.0.3",
		"@react-native-community/picker": "^1.8.0",
		"@react-native-community/push-notification-ios": "^1.5.0",
		"@react-native-community/slider": "^3.0.3",
		"@react-native-firebase/app": "^8.4.5",
		"@react-native-firebase/messaging": "^7.8.11",
		"clamp": "^1.0.1",
		"color": "^3.1.2",
		"color-parse": "^1.4.2",
		"jsc-android": "^241213.1.0",
		"lodash": "^4.17.20",
		"moment": "^2.29.4",
		"native-base": "^2.13.12",
		"react": "16.13.1",
		"react-data-hooks": "^1.14.0",
		"react-hook-form": "^6.9.0",
		"react-native": "0.63.2",
		"react-native-android-auto-update": "^0.1.0",
		"react-native-android-intent-launcher": "^0.3.0",
		"react-native-datepicker": "^1.7.2",
		"react-native-device-info": "^6.2.0",
		"react-native-drawer": "^2.5.1",
		"react-native-easy-grid": "^0.2.2",
		"react-native-exception-handler": "^2.10.8",
		"react-native-exit-app": "^1.1.0",
		"react-native-fast-image": "^8.3.2",
		"react-native-gesture-handler": "^1.6.1",
		"react-native-global-props": "^1.1.5",
		"react-native-image-view": "^2.1.9",
		"react-native-keyboard-aware-scroll-view": "^0.9.2",
		"react-native-orientation-locker": "^1.2.0",
		"react-native-permissions": "^2.2.2",
		"react-native-reanimated": "^1.8.0",
		"react-native-render-html": "^4.2.4",
		"react-native-restart": "^0.0.17",
		"react-native-router-flux": "^4.2.0",
		"react-native-screens": "^1.0.0-alpha.23",
		"react-native-share": "^4.0.1",
		"react-native-splash-screen": "^3.2.0",
		"react-native-svg": "^12.1.0",
		"react-native-timer": "^1.3.6",
		"react-native-vector-icons": "^7.1.0",
		"react-native-webview": "^10.9.2",
		"rn-fetch-blob": "^0.12.0"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/runtime": "^7.11.2",
		"@react-native-community/eslint-config": "^2.0.0",
		"babel-jest": "^26.5.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"eslint": "^7.10.0",
		"jest": "^26.5.0",
		"jetifier": "^1.6.6",
		"metro-react-native-babel-preset": "^0.63.0",
		"react-native-svg-transformer": "^0.14.3",
		"react-test-renderer": "16.13.1"
	},
	"jest": {
		"preset": "react-native"
	}
}
